        {{if (gt .Total 0)}}
        <table class="ptt" style="margin:2px auto 0px">
            <tbody>
            <tr>
                {{if (gt .Page 1)}}
                    <td {{if (ne 1 $.Page)}}onclick="document.location=this.firstChild.href" {{else}}class="ptds"{{end}}><a href="/?page={{(call $.AddI .Page -1)}}&f_search={{$.FSearch}}&f_cats={{$.FCats}}" onclick="return false"><</a></td>
                {{end}}
                {{if .GenPageFirst}}
                    <td {{if (ne 1 .Page)}}onclick="document.location=this.firstChild.href" {{else}}class="ptds"{{end}}><a href="/?page=0&f_search={{$.FSearch}}&f_cats={{$.FCats}}" onclick="return false">1</a></td>
                {{end}}
                {{if .GenPageLeftJumper}}
                    <td onclick="var page=prompt('Jump to page: (1-{{.MaxPage}})', {{.Page}}); if(page != null) document.location='/?page='+Math.min({{.MaxPage}}, Math.max(0, page - 1))+'&f_search={{$.FSearch}}&f_cats={{$.FCats}}'">
                        ...
                    </td>
                {{end}}
                {{range .Pages}}
                    <td {{if (ne . $.Page)}}onclick="document.location=this.firstChild.href" {{else}}class="ptds"{{end}}><a href="/?page={{(call $.AddI . -1)}}&f_search={{$.FSearch}}&f_cats={{$.FCats}}" onclick="return false">{{.}}</a></td>
                {{end}}
                {{if .GenPageRightJumper}}
                    <td onclick="var page=prompt('Jump to page: (1-{{.MaxPage}})', {{.Page}}); if(page != null) document.location='/?page='+Math.min({{.MaxPage}}, Math.max(0, page - 1))+'&f_search={{$.FSearch}}&f_cats={{$.FCats}}'">
                        ...
                    </td>
                {{end}}
                {{if .GenPageLast}}
                    <td {{if (ne .MaxPage .Page)}}onclick="document.location=this.firstChild.href" {{else}}class="ptds"{{end}}><a href="/?page={{(call $.AddI .MaxPage -1)}}&f_search={{$.FSearch}}&f_cats={{$.FCats}}" onclick="return false">{{.MaxPage}}</a></td>
                {{end}}
                {{if (lt .Page .MaxPage)}}
                    <td {{if (ne .MaxPage $.Page)}}onclick="document.location=this.firstChild.href" {{else}}class="ptds"{{end}}><a href="/?page={{(call $.AddI .Page -1)}}&f_search={{$.FSearch}}&f_cats={{$.FCats}}" onclick="return false">></a></td>
                {{end}}
            </tr>
            </tbody>
        </table>
        {{end}}
        <table class="itg glte">
            <tbody>

            {{range .Galleries}}
            <tr data-new="1">
                <td class="gl1e" style="width:250px">
                    <div style="height:340px;width:250px"><img
                                    style="height:356px;width:250px;top:-8px"
                                    alt="{{.Title}}"
                                    title="{{.Title}}"
                                    src="{{(call $.GenThumb .Thumb)}}">
                    </div>
                </td>
                <td class="gl2e">
                    <div>
                        <div class="gl3e">
                            <div class="cn {{index $.CategoryToCt .Category}}">{{.Category}}</div>
                            <div class="glnew"
                                 id="posted_{{.GId}}">{{.Posted.Format "2006-01-02 15:04:05"}}
                            </div>
                            <div class="ir" style="{{(call $.GenRatingStyle .Rating)}}"><div style="display:none">{{.Rating}}</div></div>
                            <div><a href="/?f_search=uploader:{{.Uploader}}$">{{.Uploader}}</a></div>
                            <div>{{.FileCount}} pages{{if .Expunged}}<br/><div style="color: red">Expunged</div>{{end}}</div>
                        </div>
                        <div class="gl4e glname" style="min-height:348px">
                            <div class="glink">{{.Title}}<br/>{{.TitleJpn}}<br/>/g/{{.GId}}/{{.Token}}</div>
                            <div>
                                <table>
                                    <tbody>
                                    {{range $key, $values := .Tags}}
                                        <tr>
                                            <td class="tc">{{$key}}:</td>
                                            <td>
                                                {{range $values}}
                                                    <a href="/?f_search={{$key}}:{{.}}$"><div class="gtl" title="{{$key}}:{{.}}">{{.}}</div></a>
                                                {{end}}
                                            </td>
                                        </tr>
                                    {{end}}
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </td>
            </tr>
            {{end}}

            </tbody>
        </table>
        {{if (gt .Total 0)}}
        <table class="ptb" style="margin:0px auto 5px">
            <tbody>
            <tr>
                {{if (gt .Page 1)}}
